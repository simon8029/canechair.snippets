			"",
			"\tconst onButtonClick_cancel = () => {",
			"\t\tsetEnabledRow(undefined);",
			"\t};",
			"",
			"\treturn (",
			"\t\t<>",
			"\t\t\t<h1>${1:__ComponentName__}Management</h1>",
			"\t\t\t<form onSubmit={formik.handleSubmit}>",
			"\t\t\t\t<TextField",
