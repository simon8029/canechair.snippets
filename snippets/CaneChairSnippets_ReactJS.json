			"\t\t\tres => {",
			"\t\t\t\tif (res.status === 201) {",
			"\t\t\t\t\tdispatch(addNew${1:__ComponentName__}Success(res.data));",
			"\t\t\t\t}",
			"\t\t\t},",
			"\t\t\terr => {",
			"\t\t\t\tdispatch(addNew${1:__ComponentName__}Failure(err));",
			"\t\t\t}",
			"\t\t);",
			"\t};",
