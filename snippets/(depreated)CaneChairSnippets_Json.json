      "\t\t\t\t[\"String\", \"Avoid using the `String` type. Did you mean `string`?\"]",
      "\t\t\t]",
      "\t\t},",
      "\t\t\"class-name\": true,",
      "\t\t\"comment-format\": [true, \"check-space\"],",
      "\t\t\"curly\": [true, \"ignore-same-line\"],",
      "\t\t\"indent\": [true, \"spaces\"],",
      "\t\t\"interface-name\": [false, \"never-prefix\"],",
      "\t\t\"interface-over-type-literal\": false,",
      "\t\t\"jsdoc-format\": true,",
      "\t\t\"linebreak-style\": [false, \"CRLF\"],",
      "\t\t\"no-inferrable-types\": false,",
      "\t\t\"no-internal-module\": true,",
      "\t\t\"no-null-keyword\": true,",
      "\t\t\"no-switch-case-fall-through\": true,",
      "\t\t\"no-trailing-whitespace\": [true, \"ignore-template-strings\"],",
      "\t\t\"no-unnecessary-qualifier\": true,",
      "\t\t\"no-var-keyword\": true,",
      "\t\t\"object-literal-shorthand\": true,",
      "\t\t\"one-line\": [true, \"check-open-brace\", \"check-whitespace\"],",
      "\t\t\"prefer-const\": true,",
      "\t\t\"quotemark\": [false, \"double\", \"avoid-escape\"],",
      "\t\t\"semicolon\": [true, \"always\", \"ignore-bound-class-methods\"],",
      "\t\t\"space-within-parens\": true,",
      "\t\t\"triple-equals\": true,",
      "\t\t\"typedef-whitespace\": [",
      "\t\t\ttrue,",
      "\t\t\t{",
      "\t\t\t\t\"call-signature\": \"nospace\",",
      "\t\t\t\t\"index-signature\": \"nospace\",",
