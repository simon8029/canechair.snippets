      "\t\t\t\t\"parameter\": \"onespace\",",
      "\t\t\t\t\"property-declaration\": \"onespace\",",
      "\t\t\t\t\"variable-declaration\": \"onespace\"",
      "\t\t\t}",
      "\t\t],",
      "\t\t\"whitespace\": [",
      "\t\t\ttrue,",
